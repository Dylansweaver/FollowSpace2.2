<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:twittersdk="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/relativeLayout">

    <!--my_tweet_activity.xml-->


    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Nasa"
        android:textSize="20dp"
        twittersdk:layout_constraintLeft_toLeftOf="parent"
        twittersdk:layout_constraintRight_toRightOf="parent"
        twittersdk:layout_constraintTop_toTopOf="parent" />

    <android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="24dp"
        twittersdk:layout_constraintLeft_toLeftOf="parent"
        twittersdk:layout_constraintStart_toStartOf="parent"
        twittersdk:layout_constraintTop_toTopOf="parent">


        <ListView
            android:id="@id/android:list"
            android:layout_width="wrap_content"
            android:layout_height="406dp"
            android:divider="#e1e8ed"
            android:dividerHeight="1dp"
            android:drawSelectorOnTop="false"
            android:footerDividersEnabled="false"
            android:headerDividersEnabled="true"
            twittersdk:layout_constraintBottom_toBottomOf="parent"
            twittersdk:layout_constraintStart_toStartOf="parent"
            twittersdk:layout_constraintTop_toTopOf="parent"
            twittersdk:layout_constraintVertical_bias="0.657" />

    </android.support.constraint.ConstraintLayout>
</android.support.constraint.ConstraintLayout>
